{
  "name": "aztack-claude-code-agents",
  "owner": {
    "name": "Wang Weihua",
    "email": "aztack@163.com",
    "url": "https://github.com/aztack"
  },
  "metadata": {
    "description": "Aztack's Personal Claude Code Agents",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "git-commit-generator",
      "source": "./agents/git-commit-generator",
      "description": "Professional Git workflow agent that analyzes code diffs and generates Conventional Commits compliant commit messages with automatic change classification",
      "version": "1.0.0",
      "author": {
        "name": "Wang Weihua",
        "url": "https://github.com/aztack"
      },
      "homepage": "https://github.com/aztack/claude-code-agents",
      "repository": "https://github.com/aztack/claude-code-agents",
      "license": "MIT",
      "keywords": [
        "git",
        "commit",
        "conventional-commits",
        "workflow"
      ],
      "category": "development",
      "strict": false,
      "commands": [
        "./commands/commit_message.md",
        "./commands/commit.md",
        "./commands/commit_all.md"
      ],
      "agents": [
        "./agents/git-commit-generator.md"
      ]
    },
    {
      "name": "d2c-generator",
      "source": "./agents/d2c-generator",
      "description": "This agent, specializing in UI development with Semi Design, transforms UI design images into high-quality, production-ready React components. It analyzes UI mockups, identifies layout structures, and generates corresponding components and styles.",
      "version": "1.0.0",
      "author": {
        "name": "Wang Weihua",
        "url": "https://github.com/aztack"
      },
      "homepage": "https://github.com/aztack/claude-code-agents",
      "repository": "https://github.com/aztack/claude-code-agents",
      "license": "MIT",
      "keywords": [
        "ui",
        "react",
        "component-generator",
        "semi-design"
      ],
      "category": "development",
      "strict": false,
      "commands": [
        "./commands/ocr.md",
        "./commands/implComponents.md"
      ],
      "agents": [
        "./agents/d2c-generator.md"
      ]
    }
  ]
}
